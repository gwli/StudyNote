///$tab Main
//Binary 招生就业分析.qvw;
SET ThousandSep=',';
SET DecimalSep='.';
SET MoneyThousandSep=',';
SET MoneyDecimalSep='.';
SET MoneyFormat='¥#,##0.00;¥-#,##0.00';
SET TimeFormat='h:mm:ss';
SET DateFormat='YYYY/M/D';
SET TimestampFormat='YYYY/M/D h:mm:ss[.fff]';
SET MonthNames='一月;二月;三月;四月;五月;六月;七月;八月;九月;十月;十一月;十二月';
SET DayNames='周一;周二;周三;周四;周五;周六;周日';


就业分析:
LOAD 学号 as 学号, 
//     left(学号,2) as 年级,
     学院 as 学院_就业, 
     姓名, 
     毕业去向, 
//     单位名称, 
     //扩展项5, 
     专业, 
//     学历, 
     性别, 
//     生源地区, 
     家庭地址, 
     毕业选择, 
     来源省份, 
//     学号文本, 
     家庭状况
//     中图分类号, 
//     主键码, 
//     借书, 
//     包括图书种类, 
//     还书, 
//     时间差
FROM
就业分析.qvd
(qvd);
Inner join

T1:
LOAD 
//	 修读学年, 
//     修读学期, 
//     取得学年, 
//     取得学期, 
      学号, 
//     课程号, 
//     课程名称, 
//     课程学分, 
//     课程属性, 
//     课程性质, 
//     课程类别, 
       avg(考试成绩) as 平均成绩,
       if( avg(考试成绩)>=85,'优',if( avg(考试成绩)>=75,'良','一般')) as 成绩分级
//     学分绩点, 
//     取得方式, 
//     辅修标记
FROM
学生成绩.qvd(qvd)
group by 学号;


Inner join
学生基本信息就业部分:
LOAD 学号,
     XM as 姓名, 
//     高考考生号, 
